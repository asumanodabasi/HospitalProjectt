@model IEnumerable<HospitalProject.Models.Klinik>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DoctorId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DoctorId)
            </td>
            <td>
                <a asp-action="Guncel" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Index" asp-route-id="@item.Id">Add</a> 
                
            </td>
        </tr>
}
    </tbody>
</table>
