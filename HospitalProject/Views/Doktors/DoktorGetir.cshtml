@model IEnumerable<HospitalProject.Models.Doctor>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Klinik)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Hospital)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Klinik.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WorkId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Hospital.Id)
            </td>

        </tr>
}
    </tbody>
</table>
